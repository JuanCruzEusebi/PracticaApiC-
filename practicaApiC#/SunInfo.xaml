<Page x:Class="practicaApiC_.SunInfo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:practicaApiC_"
      mc:Ignorable="d" 
      Background="White"
      d:DesignHeight="450" d:DesignWidth="800"       
      Title="SunInfo">

    <Grid>
    

        <StackPanel>
            <Button x:Name="loadSunInfo" Margin="20" Click="loadSunInfo_Click" >Load Sun Info</Button>
            <TextBlock x:Name="sunriseText" Margin="20"></TextBlock>
            <TextBlock x:Name="sunsetText" Margin="20"></TextBlock>
        </StackPanel>

      
    </Grid>
</Page>
